#include <stdio.h>
int main() {
 int age;
 double marks;
 printf("Enter the number & marks = ");
 scanf("%d %lf",&age,&marks);
 age = age - 20;
 marks = marks * 11.2;
 age /=2;
 age +=age;
 age %=age;
  printf(" Her age is %d and my marks is %lf",age,marks);
